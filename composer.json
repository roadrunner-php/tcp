{
    "name": "spiral/roadrunner-tcp",
    "type": "library",
    "description": "RoadRunner: TCP worker",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Buchnev / butschster",
            "email": "butschster@gmail.com"
        },
        {
            "name": "RoadRunner Community",
            "homepage": "https://github.com/spiral/roadrunner/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "spiral/roadrunner-worker": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\RoadRunner\\Tcp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\RoadRunner\\Tcp\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5.10",
        "spiral/code-style": "^1.0",
        "jetbrains/phpstorm-attributes": "^1.0",
        "vimeo/psalm": "^4.4"
    },
    "scripts": {
        "tests": "phpunit",
        "cs-fix": "spiral-cs fix src",
        "cs-check": "spiral-cs check src",
        "psalm": "psalm --no-cache"
    },
    "suggest": {
        "spiral/roadrunner-cli": "Provides RoadRunner installation and management CLI tools"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
